{
  "name": "@cieloazul310/gatsby-theme-aoi-blog-cli",
  "version": "2.1.1",
  "description": "CLI package for Gatsby Theme Aoi Blog",
  "author": "cieloazul310 <cieloazul310@gmail.com>",
  "homepage": "https://github.com/cieloazul310/gatsby-aoi#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "bin": {
    "gatsby-aoi-blog": "bin/gatsby-aoi-blog"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "bin",
    "archetypes.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cieloazul310/gatsby-aoi",
    "directory": "packages/gatsby-theme-aoi-blog-cli"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "dev": "ts-node --files src/index.ts",
    "dev:watch": "ts-node-dev --files --respawn src/index.ts new post typescript-example -a cieloazul310",
    "prepare": "npm run build",
    "eslint": "eslint src/",
    "test": "node lib/index.js"
  },
  "bugs": {
    "url": "https://github.com/cieloazul310/gatsby-aoi/issues"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "commander": "^9.0.0"
  },
  "peerDependencies": {
    "@cieloazul310/gatsby-theme-aoi-blog-utils": "2.1.0"
  }
}
