{
  "extends": "@cieloazul310/tsconfig/gatsby.json",
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "CommonJS",
      "baseUrl": ".",
      "paths": {
        "@reach/router": ["../../node_modules/@gatsbyjs/reach-router"]
      }
    }
  },
  "include": [
    "stories",
    "components",
    ".storybook/*.ts",
    ".storybook/*.tsx",
    ".eslintrc.js"
  ],
  "exclude": ["node_modules"]
}
